{
  "name": "a-eyes-monorepo",
  "version": "1.0.0",
  "scripts": {
    "start:backend:python": "cd backend && python services/subscriber_pc.py",
    "start:backend:node": "cd backend && node image_processor.js",
    "start:frontend": "cd application && npx expo start",
    "dev": "concurrently --kill-others-on-fail --interactive \"npm:start:frontend\" \"npm:start:backend:python\" \"npm:start:backend:node\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
